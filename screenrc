sessionname zenoss-$ZENREL
source $HOME/.screenrc
hardstatus on
hardstatus alwayslastline
hardstatus string "%{.bW}%-w%{.rW}%n %t%{-}%+w %=%{..G} $ZENREL "
shell zbash
shelltitle bash
chdir $ZENHOME
screen -t home /bin/bash --rcfile $ZENOSS_DEV_HOME/zenossrc
chdir $ZENSRC
screen -t core /bin/bash --rcfile $ZENOSS_DEV_HOME/zenossrc
chdir $ZENENTSRC
screen -t enterprise /bin/bash --rcfile $ZENOSS_DEV_HOME/zenossrc
select 0
